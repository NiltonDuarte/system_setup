{
    "workbench.iconTheme": "vscode-icons",
    "files.autoSave": "afterDelay",
    "extensions.ignoreRecommendations": false,
    "python.linting.flake8Enabled": true,
    "editor.wordWrap": "on",
    "git.autofetch": true,
    "tabnine.experimentalAutoImports": true,
    "cSpell.userWords": [
        "accessibilities",
        "bigquery",
        "bynode",
        "bytree",
        "cloudbuild",
        "cloudsdktool",
        "comissao",
        "Cumul",
        "databricks",
        "dataframe",
        "dialogflowcx",
        "dotz",
        "entrypoint",
        "eval",
        "Finalizer",
        "fprint",
        "FUNCS",
        "GBTREE",
        "gcloud",
        "getenv",
        "groupby",
        "inplace",
        "inversed",
        "Keras",
        "kubernetes",
        "localtime",
        "mirakl",
        "mlflow",
        "noqa",
        "Numpy",
        "openjdk",
        "ortools",
        "pylint",
        "pyspark",
        "pytest",
        "pywrapcp",
        "reponsys",
        "sklearn",
        "southamerica",
        "Struct",
        "WORKDIR",
        "zoneinfo"
    ],
    "cSpell.enabled": true,
    "cSpell.language": "en,pt-BR",
    "cSpell.enableFiletypes": [
        "dockerfile",
        "jupyter",
        "sql"
    ],
    "python.formatting.autopep8Args": [
        "--ignore",
        "E402",
        "--max-line-length=120"
    ],
    "python.linting.flake8Args": [
        "--max-line-length=120"
    ],
    "git.confirmSync": false,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.editor.untitled.hint": "hidden",
    "editor.formatOnSave": true,
    "explorer.confirmDelete": false,
    "git.suggestSmartCommit": false,
    "vsicons.dontShowNewVersionMessage": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.showStartPage": false,
    "databricks.connection.default.localSyncFolder": "/home/niltonduarte/workspace/databricks",
    "settingsSync.ignoredSettings": [
        "databricks.connection.default.apiRootUrl",
        "databricks.connection.default.displayName",
        "databricks.connection.default.localSyncFolder",
        "databricks.connection.default.localSyncSubfolders",
        "databricks.connection.default.personalAccessToken",
        "databricks.connection.default.exportFormats",
        "-python.venvPath",
        "git.rebaseWhenSync"
    ],
    "databricks.connections": [],
    "databricks.lastActiveConnection": "ADB-ID-HML",
    "databricks.connection.default.displayName": "ADB-ID-HML",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "python.venvPath": "~/.cache/pypoetry/virtualenvs/",
    "git.rebaseWhenSync": true,
    "rest-client.defaultHeaders": {
    
        "User-Agent": "vscode-restclient"
    }
}