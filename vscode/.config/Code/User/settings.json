{
    "workbench.iconTheme": "vscode-icons",
    "files.autoSave": "afterDelay",
    "extensions.ignoreRecommendations": false,
    "python.linting.flake8Enabled": true,
    "editor.wordWrap": "on",
    "git.autofetch": true,
    "cSpell.userWords": [
        "accessibilities",
        "arpack",
        "bigquery",
        "bynode",
        "bytree",
        "cloudbuild",
        "cloudsdktool",
        "comissao",
        "Cumul",
        "databricks",
        "dataframe",
        "debussy",
        "deequ",
        "deid",
        "deidentify",
        "dejson",
        "dequu",
        "dialogflowcx",
        "dont",
        "dotz",
        "entrypoint",
        "eval",
        "Feux",
        "Finalizer",
        "fprint",
        "FUNCS",
        "GBTREE",
        "gcloud",
        "getenv",
        "goog",
        "googleapis",
        "groupby",
        "howto",
        "inplace",
        "inversed",
        "jdbc",
        "jsonl",
        "Keras",
        "kubernetes",
        "localtime",
        "mirakl",
        "mlflow",
        "mssql",
        "mysql",
        "noqa",
        "Numpy",
        "openjdk",
        "ortools",
        "protobuf",
        "pydeequ",
        "pylint",
        "pyspark",
        "pytest",
        "pywrapcp",
        "rdbms",
        "reponsys",
        "sklearn",
        "southamerica",
        "Struct",
        "taskmixin",
        "WORKDIR",
        "xcom",
        "zoneinfo"
    ],
    "cSpell.enabled": true,
    "cSpell.language": "en,pt-BR",
    "cSpell.enableFiletypes": [
        "dockerfile",
        "jupyter",
        "sql"
    ],
    "python.formatting.autopep8Args": [
        "--ignore",
        "E402",
        "--max-line-length=120"
    ],
    "python.linting.flake8Args": [
        "--max-line-length=120"
    ],
    "git.confirmSync": false,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.editor.untitled.hint": "hidden",
    "editor.formatOnSave": true,
    "explorer.confirmDelete": false,
    "git.suggestSmartCommit": false,
    "vsicons.dontShowNewVersionMessage": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "settingsSync.ignoredSettings": [
        "databricks.connection.default.apiRootUrl",
        "databricks.connection.default.displayName",
        "databricks.connection.default.localSyncFolder",
        "databricks.connection.default.localSyncSubfolders",
        "databricks.connection.default.personalAccessToken",
        "databricks.connection.default.exportFormats",
        "-python.venvPath",
        "git.rebaseWhenSync"
    ],
    "editor.acceptSuggestionOnEnter": "off",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "python.venvPath": "~/.cache/pypoetry/virtualenvs/",
    "rest-client.defaultHeaders": {
        "User-Agent": "vscode-restclient"
    },
    "explorer.confirmDragAndDrop": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "projectManager.git.baseFolders": [
        "~/workspace/git"
    ],
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "files.exclude": {
        ".pytest_cache": true,
        ".vscode": true,
        "*.egg-info": true,
        "**/__pycache__": true
    },
    "python.linting.mypyEnabled": true
}